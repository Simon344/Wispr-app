<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">You've Discovered My Wisp!</h1>

    <% wispsArray.forEach(function(element){ %>
      <p class="wisp-text"><%= element.wisp %></p>
    <% }); %>
    <hr>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Wisp</a>
  </div>
</div>

<%- include('partials/footer') %>
